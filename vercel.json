{
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      },
      {
        "src": "dist/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      },
      {
        "src": "/",
        "dest": "client/dist/index.html"
      },
      {
        "src": "/(.+)",
        "dest": "client/dist/$1"
      }
    ]
  }